{
  "version": 2,
  "builds": [
    {
      "src": "sub-store.min.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/sub-store.min.js"
    }
  ],
  "installCommand": "corepack enable && pnpm install",
  "buildCommand": "pnpm run bundle:esbuild"
}