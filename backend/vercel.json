{
  "version": 2,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.js"
    }
  ],
  "functions": {
    "src/main.js": {
      "maxDuration": 30
    }
  }
}